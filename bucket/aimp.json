{
    "homepage": "http://aimp.ru",
    "version": "4.60.2156",
    "description": "AIMP is a music player and library manager",
    "url": "http://aimp.su/storage/c469ab9a1c3ff87029ddff91cca68327/aimp_4.60.2156.exe",
    "hash": "4A2EB5CD18774A2BD3EFFF6209FFC51C82997BB52FCDECC5D5F63114A643A265",
    "installer": {
        "args": ["/AUTO=$dir", "/PORTABLE", "/SILENT"],
        "keep": false
    },
    "persist": "Profile",
    "bin": "aimp.exe",
    "shortcuts": [["aimp.exe", "AIMP"]],
    "checkver": {
        "url": "http://www.aimp.ru/?do=download&os=windows",
        "regex": "\\<h1\\>AIMP v([\\d.]+), build ([\\d.]+)\\<\\/h1\\>",
        "replace": "${1}.${2}"
    },
    "autoupdate": {
        "url": "http://aimp.su/storage/$matchToken/aimp_$version.exe",
        "hash": {
            "regex": "SHA256: $sha256"
        }
    }
}
