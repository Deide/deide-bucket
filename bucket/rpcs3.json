    "description": "Playstation 3 Emulator",
    "license": {
        "identifier": "GPL-2.0",
        "url": "https://github.com/RPCS3/rpcs3/blob/master/LICENSE"
    },
    "url": "https://github.com/RPCS3/rpcs3-binaries-win/releases/download/build-7d32dc312f6ceb5ce2eecd90063dd839b478598c/rpcs3-v0.0.24-14208-7d32dc31_win64.7z",
    "hash": "5118791581d3a15153f56399e7784d214631a8caf5c355b283fd3f3e411e9588",
    "bin": "rpcs3.exe",
    "shortcuts": [
        [
            "rpcs3.exe",
            "PS3"
        ]
    ],
    "persist": [
        "dev_hdd0",
        "dev_hdd1",
        "cache",
        "config.yml",
        "config_input.yml",
        "games.yml",
        "config",
        "GuiConfigs/CurrentSettings.ini",
        "InputConfigs",
        "patches"
    ],
    "installer": {
        "script": [
            "$file = 'GuiConfigs\\CurrentSettings.ini'",
            "if (-not (Test-Path \"$persist_dir\\$file\")) {",
            "    Write-Host \"$file does not exists.\" -f Yellow",
            "    $outfile = \"$dir\\$file\"",
            "    New-Item $outFile -Type File -Force | Out-Null",
            "}"
        ]
    },
    "checkver": {
        "url": "https://update.rpcs3.net/?api=v2&c=0",
        "jsonpath": {
            "version": "$.latest_build.version",
            "download": "$.latest_build.windows.download",
            "hash": "$.latest_build.windows.checksum"
        }
    },
    "autoupdate": {
        "url": "$matchDownload",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
