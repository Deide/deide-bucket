{
    "homepage": "https://rpcs3.net/",
    "version": "0.0.27-14854",
    "description": "Playstation 3 Emulator",
    "license": {
        "identifier": "GPL-2.0",
        "url": "https://github.com/RPCS3/rpcs3/blob/master/LICENSE"
    },
    "url": "https://github.com/RPCS3/rpcs3-binaries-win/releases/download/build-6fbca1acfd5ee7da782f93995cff99dd09cfe48d/rpcs3-v0.0.27-14854-6fbca1ac_win64.7z",
    "hash": "3f86d2a85ff3c8b8b716a9984fa1e73a47917f116ef8218be042be41378853c0",
    "bin": "rpcs3.exe",
    "shortcuts": [
        [
            "rpcs3.exe",
            "PS3"
        ]
    ],
    "persist": [
        "dev_flash",
        "dev_flash2",
        "dev_flash3",
        "dev_hdd0",
        "dev_hdd1",
        "cache",
        "config",
        "patches",
        "captures",
        "recordings",
        "savestates",
        "config.yml",
        "games.yml"
    ],
    "installer": {
        "script": [
            "function create($path, $type, $content) {",
            "   if (-not (Test-Path \"$path\")) {",
            "       New-Item \"$path\" -ItemType $type -Value $content | Out-Null",
            "       Write-Output \"Creating $path...\"",
            "   }",
            "}",
            "$conf = \"$persist_dir\\config.yml\"",
            "$games = \"$persist_dir\\games.yml\"",
            "create $conf File",
            "create $games File"
        ]
    },
    "checkver": {
        "url": "https://update.rpcs3.net/?api=v2&c=0",
        "jsonpath": {
            "version": "$.latest_build.version",
            "download": "$.latest_build.windows.download",
            "hash": "$.latest_build.windows.checksum"
        }
    },
    "autoupdate": {
        "url": "$matchDownload",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
