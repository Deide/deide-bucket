{
    "homepage": "https://rpcs3.net/",
    "version": "0.0.22-13542",
    "description": "Playstation 3 Emulator",
    "license": {
        "identifier": "GPL-2.0",
        "url": "https://github.com/RPCS3/rpcs3/blob/master/LICENSE"
    },
    "url": "https://github.com/RPCS3/rpcs3-binaries-win/releases/download/build-f9a62667cf79fb10c50c233b764ffec0a8327fe7/rpcs3-v0.0.22-13542-f9a62667_win64.7z",
    "hash": "ff11edff42480092f817fc2d2d5d07f370513fbca5a5051b059ceb8630e065f5",
    "bin": "rpcs3.exe",
    "shortcuts": [
        [
            "rpcs3.exe",
            "PS3"
        ]
    ],
    "persist": [
        "dev_hdd0",
        "dev_hdd1",
        "cache",
        "config.yml",
        "config_input.yml",
        "games.yml",
        "config",
        "GuiConfigs/CurrentSettings.ini",
        "InputConfigs",
        "patches"
    ],
    "installer": {
        "script": [
            "$file = 'GuiConfigs\\CurrentSettings.ini'",
            "if (-not (Test-Path \"$persist_dir\\$file\")) {",
            "    Write-Host \"$file does not exists.\" -f Yellow",
            "    $outfile = \"$dir\\$file\"",
            "    New-Item $outFile -Type File -Force | Out-Null",
            "}"
        ]
    },
    "checkver": {
        "url": "https://update.rpcs3.net/?api=v2&c=0",
        "jsonpath": {
            "version": "$.latest_build.version",
            "download": "$.latest_build.windows.download",
            "hash": "$.latest_build.windows.checksum"
        }
    },
    "autoupdate": {
        "url": "$matchDownload",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
