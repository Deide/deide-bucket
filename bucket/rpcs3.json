{
    "homepage": "https://rpcs3.net/",
    "version": "0.0.24-14176",
    "description": "Playstation 3 Emulator",
    "license": {
        "identifier": "GPL-2.0",
        "url": "https://github.com/RPCS3/rpcs3/blob/master/LICENSE"
    },
    "url": "https://github.com/RPCS3/rpcs3-binaries-win/releases/download/build-bd1fb86492d208693d3b5b65cf5b41e26d42b6a6/rpcs3-v0.0.24-14176-bd1fb864_win64.7z",
    "hash": "b5a9a6c0afc57fdc89e10c52df61e66df8daecdd3363b836f3e8bd0969b20bcb",
    "bin": "rpcs3.exe",
    "shortcuts": [
        [
            "rpcs3.exe",
            "PS3"
        ]
    ],
    "persist": [
        "dev_hdd0",
        "dev_hdd1",
        "cache",
        "config.yml",
        "config_input.yml",
        "games.yml",
        "config",
        "GuiConfigs/CurrentSettings.ini",
        "InputConfigs",
        "patches"
    ],
    "installer": {
        "script": [
            "$file = 'GuiConfigs\\CurrentSettings.ini'",
            "if (-not (Test-Path \"$persist_dir\\$file\")) {",
            "    Write-Host \"$file does not exists.\" -f Yellow",
            "    $outfile = \"$dir\\$file\"",
            "    New-Item $outFile -Type File -Force | Out-Null",
            "}"
        ]
    },
    "checkver": {
        "url": "https://update.rpcs3.net/?api=v2&c=0",
        "jsonpath": {
            "version": "$.latest_build.version",
            "download": "$.latest_build.windows.download",
            "hash": "$.latest_build.windows.checksum"
        }
    },
    "autoupdate": {
        "url": "$matchDownload",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
