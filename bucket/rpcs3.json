{
    "homepage": "https://rpcs3.net/",
    "version": "0.0.25-14388",
    "description": "Playstation 3 Emulator",
    "license": {
        "identifier": "GPL-2.0",
        "url": "https://github.com/RPCS3/rpcs3/blob/master/LICENSE"
    },
    "url": "https://github.com/RPCS3/rpcs3-binaries-win/releases/download/build-8be4ac6869496090d9816416a9e1386e14ab5452/rpcs3-v0.0.25-14388-8be4ac68_win64.7z",
    "hash": "79aee3f6478aff520eb5c0fef93233b0e1fcbd40f87a3e02cd2dbf882c850759",
    "bin": "rpcs3.exe",
    "shortcuts": [
        [
            "rpcs3.exe",
            "PS3"
        ]
    ],
    "persist": [
        "dev_hdd0",
        "dev_hdd1",
        "cache",
        "config.yml",
        "config_input.yml",
        "games.yml",
        "config",
        "GuiConfigs/CurrentSettings.ini",
        "InputConfigs",
        "patches"
    ],
    "installer": {
        "script": [
            "$file = 'GuiConfigs\\CurrentSettings.ini'",
            "if (-not (Test-Path \"$persist_dir\\$file\")) {",
            "    Write-Host \"$file does not exists.\" -f Yellow",
            "    $outfile = \"$dir\\$file\"",
            "    New-Item $outFile -Type File -Force | Out-Null",
            "}"
        ]
    },
    "checkver": {
        "url": "https://update.rpcs3.net/?api=v2&c=0",
        "jsonpath": {
            "version": "$.latest_build.version",
            "download": "$.latest_build.windows.download",
            "hash": "$.latest_build.windows.checksum"
        }
    },
    "autoupdate": {
        "url": "$matchDownload",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
