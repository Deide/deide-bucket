{
    "homepage": "https://rpcs3.net/",
    "version": "0.0.25-14469",
    "description": "Playstation 3 Emulator",
    "license": {
        "identifier": "GPL-2.0",
        "url": "https://github.com/RPCS3/rpcs3/blob/master/LICENSE"
    },
    "url": "https://github.com/RPCS3/rpcs3-binaries-win/releases/download/build-45bd0d1f25e6f9c7f6e854f2734caa160ea8bb29/rpcs3-v0.0.25-14469-45bd0d1f_win64.7z",
    "hash": "f1b370d7aeb34cbd780360f7d0ff16964a5503d7e268fe291ee1216d2089b7cb",
    "bin": "rpcs3.exe",
    "shortcuts": [
        [
            "rpcs3.exe",
            "PS3"
        ]
    ],
    "persist": [
        "dev_hdd0",
        "dev_hdd1",
        "cache",
        "config.yml",
        "config_input.yml",
        "games.yml",
        "config",
        "GuiConfigs/CurrentSettings.ini",
        "InputConfigs",
        "patches"
    ],
    "installer": {
        "script": [
            "$file = 'GuiConfigs\\CurrentSettings.ini'",
            "if (-not (Test-Path \"$persist_dir\\$file\")) {",
            "    Write-Host \"$file does not exists.\" -f Yellow",
            "    $outfile = \"$dir\\$file\"",
            "    New-Item $outFile -Type File -Force | Out-Null",
            "}"
        ]
    },
    "checkver": {
        "url": "https://update.rpcs3.net/?api=v2&c=0",
        "jsonpath": {
            "version": "$.latest_build.version",
            "download": "$.latest_build.windows.download",
            "hash": "$.latest_build.windows.checksum"
        }
    },
    "autoupdate": {
        "url": "$matchDownload",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
